  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "#Task1-category,Title and URL\n",
    "\n",
    "titles = []\n",
    "Category = []\n",
    "URL = [] \n",
    "\n",
    "for c in container :\n",
    "    title = c.a.text\n",
    "    Categories = c.findAll(\"span\",{\"class\",\"category-name\"})\n",
    "    URLL = c.a[\"href\"]\n",
    "    \n",
    "    titles.append(title)\n",
    "    URL.append(URLL)\n",
    "    Category.append(Categories[0].text)\n",
    "\n",
    "content = {'Title' : titles , 'URL':URL , 'Category':Category}\n",
    "x = pd.DataFrame()\n",
    "df = pd.DataFrame(content, columns = ['Title','Category','URL'])\n"
   ]
  },
